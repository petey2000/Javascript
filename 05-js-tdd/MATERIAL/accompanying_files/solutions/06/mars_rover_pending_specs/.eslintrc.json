{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-alert": "off",
    "no-console": "off",
    "strict": "off",
    "no-restricted-globals": "off",
    "no-unused-vars": "off",
    "no-unused-expressions": "off",
    "no-use-before-define": "off",
    "no-param-reassign": "off"
  }
}
